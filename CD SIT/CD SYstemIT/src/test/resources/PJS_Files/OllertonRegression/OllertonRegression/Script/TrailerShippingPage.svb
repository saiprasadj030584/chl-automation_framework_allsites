Function TrailerShipping

    Call CommonFunctions.FormName("Trailer Shipping")
    set trailer = Aliases.iexplore.wndIEFrame.FrameTab.TabWindowClass.ShellDocObjectView.InternetExplorerServer.JavaPluginControlWindow.PluginEmbeddedFrame.MultipleScreenRunner.WZSTrailerShip.RootPane.null_layeredPane.null_contentPane.Panel.Panel.DCSGridFlowPanel.WizardTabbedPane.StartPanel.DCSGridFlowPanel.DCSGridFlowPanel.DCSDataField.Panel.Panel.DCSDataField_DCSTextField
    Call trailer.Keys(TrailerName)
    'Call trailer.Keys("0429")
    set Footer = Aliases.iexplore.wndIEFrame.FrameTab.TabWindowClass.ShellDocObjectView.InternetExplorerServer.JavaPluginControlWindow.PluginEmbeddedFrame.MultipleScreenRunner.WZSTrailerShip.RootPane.null_layeredPane.null_contentPane.Panel.Panel2.Panel.WizardButtonBar
    call Footer.Keys("[F7]")
    Delay(15000)
    call Footer.Keys("[Esc]")
    Delay(1000)
    Log.Message("Trailer shipping Completed")
    
End Function
